<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.DisplayController">
   <children>
      <Label layoutX="130.0" layoutY="7.0" text="Login System">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextField fx:id="tfUsername" layoutX="124.0" layoutY="78.0" promptText="Username" />
      <TextField fx:id="tfPassword" layoutX="124.0" layoutY="119.0" promptText="Password" />
      <Button layoutX="176.0" layoutY="158.0" mnemonicParsing="false" onAction="#loginAttempt" text="Login" />
   </children>
</AnchorPane>
